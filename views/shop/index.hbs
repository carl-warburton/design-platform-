{{# each profiles}}
<div class="row">
  {{# each this}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="https://yt3.ggpht.com/-zfUc8DPpBWs/AAAAAAAAAAI/AAAAAAAAAAA/DyRp7GoSxbo/s900-c-k-no-mo-rj-c0xffffff/photo.jpg" alt="..." class= "img-responsive">
      <div class="caption">
        <h3>Designer Name</h3>
        <div class="description">
          <p>{{ this.hometown}}</p>
          <a href={{ this.instagram }}>{{ this.instagram }}</a>
          <a href={{ this.twitter }}>{{ this.twitter }}</a>
        </div>
          <div class="clearfix">
            <a href="#" class="btn btn-primary pull-right" role="button">View Designs</a>
          </div>
      </div>
      </div>
    </div>
    {{/each}}
  </div>
{{/each}}
